		"typo3/cms-beuser": "~10.3.0",
		"typo3/cms-core": "~10.3.0",
		"typo3/cms-dashboard": "~10.3.0",
		"typo3/cms-extbase": "~10.3.0",
		"typo3/cms-extensionmanager": "~10.3.0",
		"typo3/cms-felogin": "~10.3.0",
		"typo3/cms-filelist": "~10.3.0",
		"typo3/cms-fluid": "~10.3.0",
		"typo3/cms-fluid-styled-content": "~10.3.0",
		"typo3/cms-form": "~10.3.0",
		"typo3/cms-frontend": "~10.3.0",
